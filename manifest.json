{
    "manifest_version": 3,
    "name": "V2 Saver",
    "version": "1.1.0",
    "description": "Effortlessly save and organize web content with tags, descriptions, and quick actions.",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "clipboardWrite"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "V2.png",
            "32": "V2.png",
            "48": "V2.png",
            "128": "V2.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+S",
                "mac": "Command+Shift+S"
            },
            "description": "Open V2 Saver"
        }
    },
    "options_page": "options.html"
}
